pipeline{
agent any
tools{
jdk 'MyJava'
maven 'mymaven'
stages{
stage ('Clonerpo'){
steps{
git 'https://github.com/PankyTcs/DevOpsClassCodes.git'
}}
stage ('Compile'){
steps {
   sh 'mvn compile'}}
stage ('Codereview'){
steps {
sh 'mvn pmd:pmd'}}
stage ('Unittesting'){
steps {
sh 'mvn test'}}

}
}


